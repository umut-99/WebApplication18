@using WebApplication18.Entities;
@model List<User>


Admin sayfasındasınız.

<h1>Kullanıcılar</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Kullanıcı Adı</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td><a href="Admin/Delete/@item.Id">Delete</a></td>
            </tr>
        }
    </tbody>
</table>
